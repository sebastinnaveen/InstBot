{
  "name": "instbot",
  "description": "A bot fullfillment service for a institution for running Node.js applications on Google Cloud Platform",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "author": "Google Inc.",
  "engines": {
    "node": ">=8"
  },
  "main": "index.js",
  "private": true,
  "cloud-repo-tools": {
    "requiresKeyFile": true,
    "requiresProjectId": true,
    "test": {
      "app": {
        "args": [
          "index.js"
        ],
        "msg": "bot fullfillment service for a institution!"
      }
    }
  },
  "scripts": {
    "start": "node index.js",
    "e2e": "repo-tools test deploy",
    "test": "repo-tools test app",
    "cover": "nyc --cache npm test; nyc report --reporter=html"
  },
  "dependencies": {
    "apiai": "^4.0.3",
    "async": "^2.6.0",
    "axios": "^0.19.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cheerio": "^1.0.0-rc.2",
    "config": "^1.30.0",
    "constants": "0.0.2",
    "express": "^4.17.1",
    "express-session": "^1.15.6",
    "express-winston": "^2.5.1",
    "firebase": "^6.0.4",
    "firebase-admin": "^8.2.0",
    "firebase-nodejs": "^1.0.1-1.1",
    "generic-pool": "^3.4.2",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.19",
    "method-override": "^2.3.10",
    "moment": "^2.24.0",
    "multer": "^1.3.0",
    "node-rest-client": "^3.1.0",
    "path": "^0.12.7",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "slack-node": "^0.1.8",
    "swagger-jsdoc": "^1.9.7",
    "util": "^0.10.3",
    "winston": "^2.4.2",
    "winston-daily-rotate-file": "^3.1.3"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.0.0",
    "ava": "^0.25.0",
    "supertest": "^4.0.0"
  }
